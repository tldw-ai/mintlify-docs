{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "tl;dw",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.png",
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": ["get-started/introduction", "get-started/quickstart"]
      },
      {
        "group": "API Reference",
        "openapi": "openapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://tldw.statuspage.io/",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://www.trytldw.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@trytldw.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.trytldw.ai/"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tldw_ai",
      "github": "https://github.com/tldw-ai/example-playbooks",
      "linkedin": "https://www.linkedin.com/company/tl-dw"
    }
  }
}
